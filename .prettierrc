{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^[a-z0-9]+",
    "",
    "^@/type",
    "^[*.schema]",
    "",
    "^@/(.*)$",
    "",
    "^@/utils/(.*)$",
    "",
    "^@ui/routes/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "tsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0"
}